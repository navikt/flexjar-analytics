{
  "name": "flexjar-analytics",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && tsc --noEmit",
    "start": "node .output/server/index.mjs",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepare": "husky"
  },
  "dependencies": {
    "@navikt/aksel-icons": "^7.35.1",
    "@navikt/ds-css": "^7.35.1",
    "@navikt/ds-react": "^7.35.1",
    "@navikt/oasis": "^4.1.1",
    "@tanstack/react-query": "^5.62.0",
    "@tanstack/react-router": "^1.140.0",
    "@tanstack/react-start": "^1.140.0",
    "@tanstack/zod-adapter": "^1.141.8",
    "dayjs": "^1.11.13",
    "nitro": "^3.0.1-alpha.1",
    "prom-client": "^15.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.14.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.5.4",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.6.0",
    "chokidar": "^4.0.3",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^16.2.7",
    "lru-cache": "^11.2.4",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=22"
  },
  "lint-staged": {
    "*": [
      "biome check --write --no-errors-on-unmatched --files-ignore-unknown=true"
    ],
    "*.{ts,tsx}": ["bash -c 'tsc --noEmit'"]
  }
}
